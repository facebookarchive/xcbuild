{
    Type = FileType;
    Identifier = text.script.ruby;
    BasedOn = text.script;

    UTI = "public.ruby-script";
    Extensions = (
        "rb",
        "rbw",
    );
    MIMETypes = (
        "text/ruby-script",
    );
    MagicWord = (
        "#!/bin/ruby",
        "#! /bin/ruby",
        "#!/usr/bin/ruby",
        "#! /usr/bin/ruby",
        "#!/usr/local/bin/ruby",
        "#! /usr/local/bin/ruby",
    );
    IsExecutable = YES;
}
