{
    Type = FileType;
    Identifier = text.script.php;
    BasedOn = text.script;

    UTI = "public.php-script";
    Extensions = (
        "php",
        "php3",
        "php4",
        "php5",
        "phtml",
    );
    MIMETypes = (
        "text/php",
        "text/x-php-script",
        "application/php",
    );
    MagicWord = (
        "#!/bin/php",
        "#! /bin/php",
        "#!/usr/bin/php",
        "#! /usr/bin/php",
        "#!/usr/local/bin/php",
        "#! /usr/local/bin/php",
    );
}
