{
    Type = FileType;
    Identifier = text.script.python;
    BasedOn = text.script;

    UTI = "public.python-script";
    Extensions = (
        "py",
    );
    MIMETypes = (
        "text/x-python-script",
    );
    MagicWord = (
        "#!/bin/python",
        "#! /bin/python",
        "#!/usr/bin/python",
        "#! /usr/bin/python",
        "#!/usr/local/bin/python",
        "#! /usr/local/bin/python",
    );
}
