{
    Type = Tool;
    Identifier = com.apple.build-tools.strip;
    Name = "Strip";

    CommandLine = "strip [options] [input]";
    RuleName = "Strip $(InputPath)";

    Outputs = (
        "$(OutputPath)",
    );

    Options = (
        {
            Name = "STRIP_STYLE";
            Type = Enumeration;
            Values = (
                "all",
                "non-global",
                "debugging",
            );
            CommandLineArgs = {
                "all" = ( );
                "non-global" = ( "-x" );
                "debugging" = ( "-S" );
            };
        },
        {
            Name = "STRIPFLAGS";
            Type = StringList;
            CommandLinePrefixFlag = "";
        },
    );
}
